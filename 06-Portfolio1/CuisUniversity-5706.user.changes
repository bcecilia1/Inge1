	        oneCommand = $b ifTrue:[
		position := position + (0@-1).
		].
	].
	
	
	direction = West ifTrue: [
		(oneCommand = $f) ifTrue: [
		position := position + (1@0).
		].
		(oneCommand = $b) ifTrue: [
		position := position - (1@0).
		].

	].
	direction = South ifTrue: [
		(oneCommand = $f) ifTrue: [
		position := position - (0@1).
		].
		(oneCommand = $b) ifTrue: [
		position := position  + (0@1).
		]
	].
	direction = East ifTrue: [
		(oneCommand = $f) ifTrue: [
		position := position - (1@0).
		].
		(oneCommand = $b) ifTrue: [
		position := position  + (1@0).
		].
	].
	(oneCommand = $r) ifTrue: [
		direction := direction right
	].
	(oneCommand = $l) ifTrue: [
		direction := direction left.
	].
