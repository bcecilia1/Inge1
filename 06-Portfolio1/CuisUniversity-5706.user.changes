test42MarsRoverFacingNorthReceivedACommandWithAnInvalidCommandAtLast
	|aMarsRover|
	
	aMarsRover := MarsRover landsAt: 4@7 facing: #North.
	
	
	self
		should: [ aMarsRover readCommands: 'rblffbt'.]
		raise: Error 
		withExceptionDo: [ :anError |
