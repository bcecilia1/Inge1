!MarsRover methodsFor: 'actions' stamp: 'CB 5/14/2023 13:18:19' prior: 50677943!
moves: oneCommand  
       oneCommand = $f ifTrue: [
	        ^state movesFoward: self. 
	]. 
       oneCommand = $b ifTrue: [
	       ^state movesBackward: self.
	].
       oneCommand = $r ifTrue: [
	       ^state := state rotateRight.
	]. 
        oneCommand = $l ifTrue: [
	       ^state := state rotateLeft.
	]. 
     ! !
